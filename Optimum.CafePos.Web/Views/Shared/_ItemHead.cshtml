@using Optimum.CafePos.Models
@model IEnumerable<ItemHead>
@{
    ViewData["Title"] = "Home Page";
}
<!--Categories Section Begin-->

<div class="container mt-2">
    <div class="row d-flex align-items-stretch mb-2">        
            <!-- Your content for the div goes here -->
            @foreach (var item in Model)
            {
            <div class="col-sm-6 col-md-2">
                <div class="card">
                    <img src=@item.imgName class="card-img-top" alt="Image 1">
                    <div class="card-body">                        
                        <p class="card-text">@item.dishHeadName</p>
                    </div>
                </div>
            </div>
            }
    </div>
</div>
<!--Categories Section End -->
