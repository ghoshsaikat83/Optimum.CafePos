@using Optimum.CafePos.Models
@model IEnumerable<ItemHead>
@{
    ViewData["Title"] = "Home Page";
}
<!--Categories Section Begin-->

<div class="container">
    <div class="row">           
            <!-- Your content for the div goes here -->
            @foreach (var item in Model)
            {
            <div class="col-6 col-sm-2 mb-4 mb-lg-0">
                <div class="card">
                    <img src=@item.imgName class="img-fluid">
                    <div class="card-body">                        
                        <p class="card-text">@item.dishHeadName</p>
                    </div>
                </div>
            </div>
            }
    </div>
</div>
<!--Categories Section End -->
